{% extends 'base.html.twig' %}
{% block title %}
	{{parent()}}Ajout Image
{% endblock %}
{% block body %}
	<div class="container-fluid">
		<div class="row justify-content-center">
			<div class="col-10 text-primary">
				<h1 class="text-center text-primary mt-4 pt-4 display-1 fw-bold">Ajout Image</h1>
				{{ form_start(form) }}
				{{ form_row(form.image) }}
				{{ form_row(form.jeux) }}
				{{ form_row(form.ajouter) }}
				{{ form_end(form) }}
			</div>
		</div>
	</div>
{% endblock %}